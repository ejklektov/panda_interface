        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-12">
                            <form role="form">
                                <div class = "modify">
                                    <label>Modify</label>
                                    <input ng-model="new.question" class="form-control" placeholder="{{new.question}}" type="text">
                                    <br>
                                    <input ng-model="new.answer" class="form-control" placeholder="{{new.answer}}" type="text">
                                    <br>
                                    <input style="float:right" class = "btn btn-default" type="button" ng-click="send_FAQ(new._id)" value = "확인">
                                </div>
                                <br>
                                <br>
                                <div ng-repeat = "FAQ in FAQS" >

                                    <!--<button class="btn btn-default" ng-click="send_FAQ(FAQ._id)" ng-model = "modify">수정</button>-->

                                    <div class="form-group">
                                        <label>Question </label>
                                        <br><div style="margin-left:20px">{{FAQ.question}}</div>
                                        <br><label>Answer</label>
                                        <br><div style="margin-left:20px">{{FAQ.answer}}</div>
                                        <!--<br><div style="margin-left:20px">{{FAQ._id}}</div>-->
                                        <!--<button class="btn btn-default"  ><a href="/admin/modify_FAQ?id={{FAQ._id}}">수정</a></button>-->
                                        <br>
                                        <!--Click me: <input type="checkbox" ng-model="checked" aria-label="Toggle ngHide"><br/>-->
                                        <!--<div>-->
                                            <!--Show:-->
                                            <!--<div class="check-element animate-show" ng-show="checked">-->
                                                <!--<span class="glyphicon glyphicon-thumbs-up"></span> I show up when your checkbox is checked.-->
                                            <!--</div>-->
                                        <!--</div>-->
                                        <!--<div>-->
                                            <!--Hide:-->
                                            <!--<div class="check-element animate-show" ng-hide="checked">-->
                                                <!--<span class="glyphicon glyphicon-thumbs-down"></span> I hide when your checkbox is checked.-->
                                            <!--</div>-->
                                        <!--</div>-->
                                        <!--<input type="checkbox" ng-model="modify">-->
                                        <div class = "modify_confirm" style="float:right">
                                            <input class = "btn btn-default" type="button" ng-model="modify" ng-click="modify_FAQ(FAQ._id)" value = "수정">
                                            <input class = "btn btn-default" type="button" ng-model="modify" ng-click="delete_FAQ(FAQ._id)" value = "삭제">
                                        </div>


                                    </div>

                                    <!--<button type="reset" class="btn btn-default">취소</button>-->
                                    <br>
                                    <br>
                                </div>
                                <div class = "add">
                                    <label>Question </label>
                                    <input ng-model="addFAQ.question" class="form-control" type="text">
                                    <br><label>Answer</label>
                                    <input ng-model="addFAQ.answer" class="form-control" type="text">
                                    <br>
                                    <input class = "btn btn-default" type="button" ng-click="add_FAQ()" value = "추가" style = "float : right">
                                </div>
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
