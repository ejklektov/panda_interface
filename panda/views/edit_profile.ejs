                        <!--<a href="/mypage"><i class="fa fa-user"></i> Profile</a>-->
                        <!--</li>-->
                        <!--<li class="list-group-item">-->
                        <!--<a href="#buy_product_list"><i class="fa fa-history"></i> 구매한 물품</a>-->
                        <!--</li>-->
