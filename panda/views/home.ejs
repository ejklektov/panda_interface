<!--Reg-Form -->
<div class="cube-portfolio container">
    <div class="row margin-bottom-30 margin-top-20">
        <div class="col-md-8">
            <form action="#" id="sky-form4" class="sky-form" >
                <header>Your words</header>
                <fieldset>
                    <section>
                        <label class="input">
                            <!--click 시 실행되어야 할 것 추가되어야 함.-->
                            <i class="icon-append fa fa-plus" style = "
padding-left: 0px;
padding-right: 0px;
"
                               ng-click = "addWord()"
                            ></i>

                            <input type="text" name="word" placeholder="Word">
                            <!--<b class="tooltip tooltip-bottom-right">Needed to enter the website</b>-->
                        </label>
                    </section>
                </fieldset>
            </form>
        </div>

        <div class="cbp-item etc" style = "width: 300px; height : 150px">
            <div class="cbp-item-wrapper">
                <div class="cbp-caption margin-bottom-20" style = "max-height : 100%; max-width : 100%">
                    <div class="cbp-caption-defaultWrap">
                        <img src="" alt="App download">
                    </div>
                    <div class="cbp-caption-activeWrap">
                        <div class="cbp-l-caption-alignCenter">
                            <div class="cbp-l-caption-body">
                                <ul class="link-captions no-bottom-space">
                                    <li><a href="/"><i class="rounded-x fa fa-link"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--End Reg-Form -->

